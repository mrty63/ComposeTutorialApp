@page
@model WebApp.Pages.AddItemModel
@{
}

<form class="form-horizontal" method="post">
    <div class="form-group">
        <label for="m_title" class="col-sm-4 control-label">Job Title</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" name="m_title">
        </div>
        <label for="m_company" class="col-sm-4 control-label">Company Name</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" name="m_company">
        </div>
    </div>
    <div class="form-group">
        <label for="m-start" class="col-sm-2 control-label">Start Date</label>
        <div class="col-sm-10">
            <input type="date" class="form-control" name="m_start">
        </div>
        <label for="m_end" class="col-sm-2 control-label">Start Date</label>
        <div class="col-sm-10">
            <input type="date" class="form-control" name="m_end">
        </div>
    </div>
    <div class="form-group">
        <div class="col-sm-10">
            <button type="submit" class="btn btn-default btn-outline-primary">Submit</button>
        </div>
    </div>
</form>


<div id="input">
    @Model.resultOfPost <br />
    @Model.resultOfDatePost

</div>